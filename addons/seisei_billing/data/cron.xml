<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Cron job to reconcile entitlements every 30 minutes -->
        <record id="ir_cron_reconcile_entitlements" model="ir.cron">
            <field name="name">Seisei: Reconcile Tenant Entitlements</field>
            <field name="model_id" ref="model_seisei_tenant"/>
            <field name="state">code</field>
            <field name="code">model.cron_reconcile_entitlements()</field>
            <field name="interval_number">30</field>
            <field name="interval_type">minutes</field>
            <field name="active">True</field>
        </record>
    </data>
</odoo>
